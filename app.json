      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#6366f1"
      },
      "package": "com.lingotoday.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-notifications"
    ],
    "scheme": "lingotoday",
    "sdkVersion": "54.0.0",
    "extra": {
      "eas": {
        "projectId": "b5c4ecbe-0f0f-4f0f-8f8e-b155460d2f03"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b5c4ecbe-0f0f-4f0f-8f8e-b155460d2f03"
    }
  }
